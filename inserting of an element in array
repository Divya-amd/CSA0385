#include<stdio.h>
int main()
{
int arr[5];
int i,x,pos,n=5;
for(i=0;i<n;i++)
{
arr[i]=i+1;
}
for(i=0;i<n;i++)
printf("%d",arr[i]);
	printf("\n");
	x=7;
	pos=3;
	n++;
	for(i=n-1;i>=pos;i--)
	{
	arr[i]=arr[i-1];
	arr[pos-1]=x;
	}
	for(i=0;i<n;i++)
	{
	printf("%d",arr[i]);
	printf("/n");
	}
	return 0;
	}
